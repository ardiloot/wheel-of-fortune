DOMAIN_NAME=example.com
HOSTNAME=wheelmetrics
DATA_DIR=/home/ubuntu/data

# Variables for Letsencrypt ACME DNS challenge (Cloudflare in this example)
LETSENCRYPT_EMAIL=
CLOUDFLARE_EMAIL=
CLOUDFLARE_DNS_API_TOKEN=

# Traefik HTTP authentication
# Use htpasswd or https://hostingcanada.org/htpasswd-generator/ (Bcrypt)
TRAEFIK_DASHBOARD_AUTH=
LOKI_AUTH=

# Generated from influxdb (can be added after setting up influxdb)
TELEGRAF_INFLUXDB_TOKEN=
TELEGRAF_INFLUXDB_ORG=wheelmetrics
TELEGRAF_INFLUXDB_BUCKET=telegraf